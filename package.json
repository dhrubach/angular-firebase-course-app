{
	"name": "angular-firebase-course-app",
	"version": "0.0.1",
	"description": "simple course listing app using angular and firebase",
	"scripts": {
		"rimraf": "./node_modules/.bin/rimraf",
		"webpack": "./node_modules/.bin/webpack",
		"ts-node": "./node_modules/.bin/ts-node",

		"refresh-data": "npm run ts-node -- --project ./server/tsconfig.server.json ./server/populate-firebase.ts",
		"clear-data": "npm run ts-node -- --project ./server/tsconfig.server.json ./server/clear-firebase.ts",

		"build": "npm run build:dev",
		"build:dev": "npm run rimraf -- dist && cross-env NODE_ENV=development npm run webpack -- --colors --progress --display-error-details --display-cached",
		"build:prod": "npm run rimraf -- dist && cross-env NODE_ENV=production npm run webpack -- --colors",
		"build:prod:analyze": "npm run rimraf -- dist && cross-env NODE_ENV=production ANALYZE=true npm run webpack -- --colors",
		"build:test": "npm run rimraf -- dist && cross-env NODE_ENV=test npm run webpack -- --colors",
		"build:watch": "npm run build -- --watch",
		"build:server": "./node_modules/.bin/webpack-dev-server --debug --colors --config ./webpack.config.js",

		"serve": "./node_modules/.bin/http-server ./dist -p 8080 -d false -o",
		"serve:prod": "./node_modules/.bin/http-server ./dist -p 8080 -d false -g -o"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dhrubach/angular-firebase-course-app.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dhrubach/angular-firebase-course-app/issues"
	},
	"homepage": "https://github.com/dhrubach/angular-firebase-course-app#readme",
	"dependencies": {
		"@angular/animations": "^4.3.6",
		"@angular/cdk": "^2.0.0-beta.10",
		"@angular/common": "^4.3.6",
		"@angular/compiler": "^4.3.6",
		"@angular/compiler-cli": "^4.3.6",
		"@angular/core": "^4.3.6",
		"@angular/forms": "^4.3.6",
		"@angular/http": "^4.3.6",
		"@angular/material": "^2.0.0-beta.10",
		"@angular/platform-browser": "^4.3.6",
		"@angular/platform-browser-dynamic": "^4.3.6",
		"@angular/router": "^4.3.6",
		"angularfire2": "^4.0.0-rc.2",
		"babel-runtime": "^6.26.0",
		"core-js": "^2.5.0",
		"firebase": "^4.3.0",
		"hammerjs": "^2.0.8",
		"lodash-es": "^4.17.4",
		"moment": "^2.18.1",
		"normalize.css": "^7.0.0",
		"promise-polyfill": "6.0.2",
		"rxjs": "^5.4.3",
		"zone.js": "^0.8.17"
	},
	"devDependencies": {
		"@types/chalk": "^0.4.31",
		"@types/lodash-es": "^4.14.6",
		"@types/webpack": "^3.0.10",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-angular2": "0.0.2",
		"babel-preset-env": "^1.6.0",
		"chalk": "^2.1.0",
		"compression-webpack-plugin": "^1.0.0",
		"cross-env": "^5.0.5",
		"css-loader": "^0.28.5",
