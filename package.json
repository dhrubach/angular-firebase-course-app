		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"fork-ts-checker-webpack-plugin": "^0.2.7",
		"friendly-errors-webpack-plugin": "^1.6.1",
		"html-webpack-plugin": "^2.30.1",
		"http-server": "^0.10.0",
		"imports-loader": "^0.7.1",
		"loader-utils": "^1.1.0",
		"ng-router-loader": "^2.1.0",
